		"@tsconfig/svelte": "^5.0.4",
		"@types/node": "^20.14.11",
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.39",
		"svelte": "^5.0.0-next.191",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.6",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^5.3.4",
		"vite-plugin-singlefile": "^2.0.2"
	},
	"dependencies": {}
}
