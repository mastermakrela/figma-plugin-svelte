{
	"name": "figma-plugin-svelte",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:ui": "vite build --watch",
		"dev:plugin": "vite build -c vite.config.plugin.ts --watch",
